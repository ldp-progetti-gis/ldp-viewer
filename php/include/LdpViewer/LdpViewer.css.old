

/* ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ *
 * ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ *
 *                                                                                                      *
 *        STILI GENERALI DELLA PAGINA                                                                   *
 *                                                                                                      *
 * ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ */

	
	body, html {
		height:						100%;
		margin:						0px;
	}
	
	/* Div nella pagina del progetto in cui viene sparato il visualizzatore mobile */			
	#ldpviewer {
		width: 						100%;
		height: 					100%;
		position: 					relative;
	}
	
	#ldpviewer_container {
		height: 100%;
	}
	
	

/* ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ *
 * ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ *
 *                                                                                                      *
 *        TOOLBAR DELLA MAPPA (versione mobile)                                                         *
 *                                                                                                      *
 * ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ */
	

	/* Toolbar con i comandi della mappa */
	#maptoolbar {
		position: 					absolute;
		top: 						5px;
		left: 						5px;
		width: 						auto;
		z-index: 					10000;
	}
	
	/* Div che contiene uno o pi√π comandi della toolbar */
	#maptoolbar div {
		background: 				gray;
		opacity: 					.8;
		padding: 					5px;
		border-radius: 				4px 4px 4px 4px;
		margin: 					5px 0;
	}
						
	#maptoolbar img {
		margin: 					0;
		display: 					block;
		border-radius: 				3px 3px 3px 3px;
		opacity: 					1;
	}
				
	#maptoolbar img.first {
		margin: 					0 0 10px 0;
	}
	
	#maptoolbar img.last {
		margin: 					0;
	}
				
	#maptoolbar img:active {
		background-color: 			#B9D1FF;
	}
	




/* ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ *
 * ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ *
 *                                                                                                      *
 *        HEADER DELLA MAPPA (versione standard)                                                       *
 *                                                                                                      *
 * ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ */

	#ldpviewer_header {
		width						: 100%;
		background					: #DDD;
		/*z-index						: 2;*/
		height						: 10%;
		position					: relative;
	}
	
	#ldpviewer_toolbar {
		background-color			: #DDD;
		width						: 100%;
		height						: 4%;
		/*position					: absolute;*/
		/*bottom						: 0px;*/
	}
	
	#ldpviewer_toolbar img {
		border						: 1px solid #DDD;
		background-repeat			: no-repeat;
	}
	
	#ldpviewer_toolbar img.disabled {
		border						: 1px solid red;
	}
	
	#ldpviewer_toolbar img.active {
		background					: #999;
		opacity						: 0.85;
	}
	
	#ldpviewer_toolbar img:hover {
		border						: 1px solid black;
	}
	
	#ldpviewer_toolbar .ldpviewer_toolbar_view_layers {
		background-image			: url('/include/img/LdpViewer/toolbar/icon_view_layers.gif');
	}
	#ldpviewer_toolbar .ldpviewer_toolbar_view_layers_off {
		background-image			: url('/include/img/LdpViewer/toolbar/icon_view_layers_off.gif');
	}
	
	#ldpviewer_toolbar .ldpviewer_toolbar_view_apps {
		background-image			: url('/include/img/LdpViewer/toolbar/icon_view_apps.gif');
	}
	#ldpviewer_toolbar .ldpviewer_toolbar_view_apps_off {
		background-image			: url('/include/img/LdpViewer/toolbar/icon_view_apps_off.gif');
	}
	
	#ldpviewer_title {
		width						: 100%;
		height						: 5%;
		overflow					: hidden;
		background-image			: url('/include/img/bkg_testata.jpg');
		display						: inline-block;
	}
	
	@media screen and (max-width: 600px) and (min-width: 200px) {
		#ldpviewer_toolbar {
			height						: 12px;
		}
		#ldpviewer_toolbar img {
			margin						: 1px;
			padding						: 1px;
			background-position			: 1px;
			background-size				: 8px;
			width						: 8px;
			height						: 8px;
		}
	}
	
	@media screen and (max-width: 1000px) and (min-width: 601px) {
		#ldpviewer_toolbar {
			height						: 20px;
		}
		#ldpviewer_toolbar img {
			margin						: 1px;
			padding						: 2px;
			background-position			: 2px;
			background-size				: 12px;
			width						: 12px;
			height						: 12px;
		}
	}
	
	@media screen and (min-width: 1001px) {
		#ldpviewer_toolbar img {
			padding						: 5px;
			background-position			: 5px;
			background-size				: 16px;
			width						: 16px;
			height						: 16px;
		}
	}
	
	
/* *************************************************** *
 *                 INTESTAZIONE                        *
 * *************************************************** */
	
	#ldpviewer_title div#intesta {
		float						: left; 
		margin						: 0;
		padding						: 0;
	}
	
	#ldpviewer_title p.nome-comune {
		margin						: 0 0 .3em 0;
		padding						: 3px 0 0 0;
		float						: left;
		width						: auto;
	}
	
	#ldpviewer_title h1 {
		font-size					: 1em; 
		margin						: 0 0 0 20px;
		text-transform				: uppercase;
		background-image			: none;
	}
	
	#ldpviewer_title h1 span {
		text-transform				: capitalize; 
		font-style					: italic;
		font-size					: .8em;
	}
	
	#ldpviewer_title img#logo {
		float						: left;
		font-size					: 0.6em;
		margin						: 10px 0 10px 0px;
		padding						: 0;
		height						: 20px;
	}
	
	#ldpviewer_title a.exit:link {
		background-image:url('/include/img/back1.gif');
		background-position: center left;
		background-repeat: no-repeat;
	}
	
	#ldpviewer_title a.exit {
		background-image:url('/include/img/back1.gif');
		background-position: center left;
		background-repeat: no-repeat;
		padding: 0 0 0 20px;
		margin: 0;
		text-decoration: none;
		color: #00601d;
		font-weight: bold;
		font-size: .8em;
	}
	
	#ldpviewer_title a.exit:visited {
		background-image:url('/include/img/back1.gif');
	}
	
	#ldpviewer_title a.exit:hover {
		background-image:url('/include/img/back2.gif');
		text-decoration: underline;
	}
	
	#ldpviewer_title a.exit:active {
		background-image:url('/include/img/back1.gif');
	}
	


